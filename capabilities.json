{
    "dataRoles": [{
            "name": "Groups",
            "kind": "Grouping",
            "displayName": "Category"
        },
        {
            "name": "Samples",
            "kind": "Grouping",
            "displayName": "Sampling"
        },
        {
            "name": "Values",
            "kind": "Measure",
            "cartesianKind": 1,
            "displayName": "Values"
        }
    ],
    "supportsHighlight": true,
    "dataViewMappings": [{
        "conditions": [{
            "Groups": {
                "min": 0,
                "max": 1
            },
            "Values": {
                "min": 0,
                "max": 1
            }
        }],
        "categorical": {
            "categories": {
                "for": {
                    "in": "Groups"
                },
                "dataReductionAlgorithm": { "top": { "count": 100 } }
            },
            "values": {
                "group": {
                    "by": "Samples",
                    "dataReductionAlgorithm": { "top": { "count": 30000 } },
                    "select": [{
                        "for": {
                            "in": "Values"   
                        }
                    }]
                }
            }
        }
    }],
    "objects": {
        "general": {
            "displayName": "General",
            "properties": {
                "formatString": {
                    "type": { "formatting": { "labelDisplayUnits": false } }
                },
                "telemetry": {
                    "displayName": "Telemetry",
                    "description": "Enable telemetry logging",
